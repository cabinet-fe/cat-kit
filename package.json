{
  "name": "fe-dk",
  "version": "1.0.15",
  "description": "前端开发工具包",
  "scripts": {
    "prepare": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "dk:build": "tsx build/index.ts",
    "test": "vitest run",
    "commit-lint": "tsx scripts/verifyCommit.ts",
    "release": "pnpm test && pnpm dk:build && cd dist && npm publish --registry https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/fe-dk.git"
  },
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./cjs/index.js",
      "types": "./es/index.d.ts"
    },
    "./es/*": "./es/*.js",
    "./cjs/*": "./cjs/*.js",
    "./*": "./*"
  },
  "homepage": "https://github.com/cabinet-fe/fe-dk#readme",
  "types": "es/index.d.ts",
  "keywords": [
    "前端开发工具包",
    "fe-dk",
    "前端文档"
  ],
  "author": "Yuan He Team FE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinet-fe/fe-dk/issues"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.2",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "rollup": "^2.77.3",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.6",
    "vitepress": "1.0.0-draft.8",
    "vitest": "^0.21.1",
    "vue": "^3.2.37",
    "chalk": "^4.1.2"
  }
}
