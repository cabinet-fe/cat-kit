{
  "name": "cat-kit",
  "version": "3.4.13",
  "description": "前后端一体开发工具包",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:server": "node docs/.vitepress/server",
    "docs:build": "vitepress build docs",
    "kit:build": "tsx build/index.ts",
    "test": "vitest run",
    "kit:build-v2": "tsx build/index.ts v2",
    "commit-lint": "tsx scripts/verifyCommit.ts",
    "release": "pnpm test && pnpm kit:build && cd dist && npm publish --registry=https://registry.npmjs.org --access=public",
    "release:v2": "pnpm test && pnpm kit:build-v2 && cd dist && npm publish --registry=https://registry.npmjs.org --access=public",
    "upgrade": "tsx scripts/version.ts upgrade",
    "downgrade": "tsx scripts/version.ts downgrade"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/cat-kit.git"
  },
  "exports": {
    ".": {
      "import": {
        "node": "./be/es/index.js",
        "default": "./fe/es/index.js"
      },
      "require": {
        "node": "./be/cjs/index.js",
        "default": "./fe/cjs/index.js"
      },
      "types": {
        "node": "./be/index.d.ts",
        "default": "./fe/index.d.ts"
      }
    },
    "./fe": {
      "import": "./fe/es/index.js",
      "require": "./fe/cjs/index.js",
      "types": "./fe/index.d.ts"
    },
    "./be": {
      "import": "./be/es/index.js",
      "require": "./be/cjs/index.js",
      "types": "./be/index.d.ts"
    },
    "./canvas": {
      "import": "./canvas/es/index.js",
      "require": "./canvas/cjs/index.js",
      "types": "./canvas/index.d.ts"
    }
  },
  "types": "fe/index.d.ts",
  "homepage": "https://github.com/cabinet-fe/cat-kit#readme",
  "keywords": [
    "ts开发工具包",
    "cat-kit",
    "技术文档"
  ],
  "author": "Yuan He Team FE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinet-fe/cat-kit/issues"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.6.0",
    "@types/prettier": "^3.0.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "picocolors": "^1.0.0",
    "prettier": "3.0.3",
    "rollup": "^3.29.1",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "terser": "5.19.4",
    "ts-conf-base": "^2.1.0",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitepress": "1.0.0-rc.13",
    "vitest": "^0.34.4",
    "vue": "^3.3.4"
  }
}
