{
  "name": "fe-dk",
  "version": "1.0.15",
  "description": "前端开发工具包",
  "scripts": {
    "prepare": "husky install",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "dk:build": "tsx build/index.ts",
    "test": "vitest run",
    "commit-lint": "tsx scripts/verifyCommit.ts",
    "release": "pnpm test && pnpm dk:build && cd dist && npm publish --registry https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/fe-dk.git"
  },
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./cjs/index.js",
      "types": "./es/index.d.ts"
    },
    "./es/*": "./es/*.js",
    "./cjs/*": "./cjs/*.js",
    "./*": "./*"
  },
  "homepage": "https://github.com/cabinet-fe/fe-dk#readme",
  "types": "es/index.d.ts",
  "keywords": [
    "前端开发工具包",
    "fe-dk",
    "前端文档"
  ],
  "author": "Yuan He Team FE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinet-fe/fe-dk/issues"
  },
  "devDependencies": {
    "utils": "workspace:*",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.9",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.1",
    "rollup": "^3.2.5",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "tsx": "^3.11.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitepress": "1.0.0-alpha.28",
    "vitest": "^0.22.1",
    "vue": "^3.2.41",
    "chalk": "^4.1.2",
    "terser": "5.15.1"
  }
}
