{
  "extends": "@cat-kit/tsconfig/tsconfig.vue.json",
  "compilerOptions": {
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "tsBuildInfoFile": "./node_modules/.cache/.tsbuildinfo"
  },
  "include": [
    ".vitepress/**/*.ts",
    ".vitepress/**/*.vue",
    "examples/**/*.vue",
    "examples/**/*.ts",
    "content/**/*.d.ts"
  ]
}
