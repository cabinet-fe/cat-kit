{
  "extends": "ts-conf-base/tsconfig.vue.json",
  "compilerOptions": {
    "composite": true,
    "incremental": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "types": ["vite/client"],
    "tsBuildInfoFile": "./node_modules/.cache/.tsbuildinfo"
  },
  "include": [
    ".vitepress/**/*.ts",
    ".vitepress/**/*.vue",
    "examples/**/*.vue",
    "examples/**/*.ts",
    "*.d.ts"
  ]
}
